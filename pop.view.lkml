  dimension: date {
    type: date
    datatype: date
    sql: ${TABLE}.date ;;
  }
  dimension_group: created {
    type: time
    view_label: "_PoP"
    timeframes: [
      raw,
      time,
      hour_of_day,
      date,
      day_of_week,
      day_of_week_index,
      day_of_month,
      day_of_year,
      week,
      week_of_year,
      month,
      month_name,
      month_num,
      quarter,
      year
    ]
    sql: ${TABLE}.date;;
    convert_tz: no
  }
  dimension: MTD {
    type: yesno
    sql: EXTRACT(Month from ${created_date}) < EXTRACT(Month from CURRENT_DATE);;
  }
  dimension: YTD {
    type: yesno
   # sql: DAYOFYEAR(${created_date}) < DAYOFYEAR(CURRENT_DATE);;
    sql: EXTRACT(year from ${created_date}) < EXTRACT(year from CURRENT_DATE);;
  }
  ##dimension: is_before_qtd {
   ## type: yesno
   ## sql: (datepart(Quater from ${created_date}) < datepart(Quater from CURRENT_DATE));;
  ##}

  dimension: QTD {
    type: yesno
    sql: EXTRACT(quarter from ${created_date}) < EXTRACT(quarter from CURRENT_DATE);;
  }


  dimension: sales {
    type: number
    sql: ${TABLE}.sales ;;
  }
  dimension: is_ytd {
    type: yesno
    group_label: "Date Restrictions"
    label: "Is YTD?"
    view_label: "Dynamic Grouping & Time Comparisons"
    sql: MONTH(${created_date}) < MONTH(CURRENT_TIMESTAMP) OR
    (MONTH(${created_date}) = MONTH(CURRENT_TIMESTAMP) AND
    DAY(${created_date}) <= DAY(CURRENT_TIMESTAMP)) ;;
    }
measure: sum_sales {
type: sum
sql: ${sales} ;;
}
  set: detail {
    fields: [date, sales]
  }
  dimension: FirstDayofQ {
    type: number
    sql:  CASE WHEN GetQuarter(${created_date}) = 1 THEN ${created_date}(getyear(${created_date}),1,1)
WHEN GetQuarter(${created_date}) = 2 THEN ${created_date}(getyear(${created_date}),4,1)
WHEN GetQuarter(${created_date}) = 3 THEN ${created_date}(getyear(${created_date}),7,1)
WHEN GetQuarter(${created_date}) = 4 THEN ${created_date}(getyear(${created_date}),10,1)
END;;
  }
  dimension: QTDMonthInd {
    type: number
 sql: CASE WHEN GetMonth(${created_date}) IN(1,4,7,10) THEN 1
WHEN GetMonth(${created_date}) IN(2,5,8,11) THEN 2
WHEN GetMonth(${created_date}) IN(3,6,9,12) THEN 3 END;;
  }
  dimension: QTDMonthIndNow {
    type: number
    sql: CASE WHEN GetMonth(now()) IN(1,4,7,10) THEN 1
WHEN GetMonth(now()) IN(2,5,8,11) THEN 2
WHEN GetMonth(now()) IN(3,6,9,12) THEN 3 END ;;
  }
  dimension: isQTD {
    type: number
   sql:CASE WHEN
(QTDMonthInd < QTDMonthIndNow and ${created_date} >= FirstDayofQ) or
(QTDMonthInd = QTDMonthIndNow AND getday(${created_date})<=getday(now()))
THEN 1 ELSE 0 END;;
  }

  parameter: date_selector {
    group_label: "YTD,QTD,MTD,WTD"
    type: date_time
    description: "Use this field to select a date to filter results by."
  }



  dimension: year_to_date_dimension_liquid {
    group_label: "YTD,QTD,MTD,WTD"
    sql: CASE
      WHEN EXTRACT( YEAR FROM ${created_date}) = EXTRACT( YEAR FROM {% parameter date_selector %})
      AND ${created_date} < {% parameter date_selector %}
      THEN 'Year to Date'
      END
      ;;
  }



  dimension: Quarter_to_date_dimension_liquid {
    group_label: "YTD,QTD,MTD,WTD"
    sql: CASE WHEN EXTRACT( YEAR FROM ${created_date}) = EXTRACT( YEAR FROM {% parameter date_selector %})
      AND EXTRACT( QUARTER FROM ${created_date}) = EXTRACT( QUARTER FROM {% parameter date_selector %})
      AND ${created_date} < {% parameter date_selector %} THEN 'Quarter to Date'
      END
      ;;
  }



  dimension: month_to_date_dimension_liquid {
    group_label: "YTD,QTD,MTD,WTD"
    sql: CASE WHEN EXTRACT( YEAR FROM ${created_date}) = EXTRACT( YEAR FROM {% parameter date_selector %})
      AND EXTRACT( MONTH FROM ${created_date}) = EXTRACT( MONTH FROM {% parameter date_selector %})
      AND ${created_date} < {% parameter date_selector %} THEN 'Month to Date'
      END
      ;;
  }
